{
  "name": "jiaojiao",
  "version": "0.0.7",
  "description": "基于 deepagents.js 的有声绘本智能生成应用",
  "author": "",
  "main": "dist-electron/main.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && electron-builder",
    "preview": "vite preview",
    "electron:dev": "vite",
    "electron:build": "node -e \"try{require('fs').rmSync('release',{recursive:true,force:true});}catch(e){}\" && tsc && vite build && electron-builder",
    "electron:build:verbose": "node -e \"try{require('fs').rmSync('release',{recursive:true,force:true});}catch(e){}\" && tsc && vite build && cross-env DEBUG=electron-builder* electron-builder",
    "test": "vitest",
    "test:run": "vitest run",
    "test:inference": "vitest run tests/integration/inference",
    "test:inference:zhipu": "npx cross-env TEST_API_PROVIDER=zhipu vitest run tests/integration/inference",
    "test:inference:dashscope": "npx cross-env TEST_API_PROVIDER=dashscope vitest run tests/integration/inference",
    "test:tools": "vitest run tests/integration/tools",
    "test:tools:zhipu": "npx cross-env TEST_API_PROVIDER=zhipu vitest run tests/integration/tools",
    "test:tools:dashscope": "npx cross-env TEST_API_PROVIDER=dashscope vitest run tests/integration/tools",
    "test:agent": "vitest run tests/integration/agent",
    "lint": "eslint .",
    "format": "prettier --write .",
    "gen-icons": "node scripts/gen-icons.mjs",
    "gen-favicon": "node scripts/gen-favicon.mjs",
    "backup:userdata": "powershell -ExecutionPolicy Bypass -File scripts/backup-userdata-jiaojiao.ps1"
  },
  "dependencies": {
    "@langchain/core": "^1.1.0",
    "@langchain/langgraph": "^1.1.0",
    "@langchain/openai": "^1.1.0",
    "@radix-ui/colors": "^1.0.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@types/js-yaml": "^4.0.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^1.2.1",
    "deepagents": "^1.3.0",
    "dotenv": "^16.4.5",
    "electron-store": "^10.0.0",
    "express": "^4.21.0",
    "fast-glob": "^3.3.2",
    "js-yaml": "^4.1.1",
    "langchain": "^1.2.0",
    "lucide-react": "^0.539.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.1",
    "react-resizable-panels": "^3.0.6",
    "sharp": "^0.33.5",
    "tailwind-merge": "^2.6.0",
    "use-stick-to-bottom": "^1.1.1",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "electron": "^32.0.0",
    "electron-builder": "^25.1.8",
    "icon-gen": "^5.0.0",
    "eslint": "^9.0.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "postcss": "^8.4.40",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.7",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.21.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.2",
    "vite-plugin-electron": "^0.28.0",
    "vitest": "^1.6.0",
    "wait-on": "^8.0.0"
  },
  "overrides": {
    "@langchain/core": "^1.1.0",
    "langchain": "^1.2.0",
    "@langchain/openai": "^1.1.0",
    "@langchain/langgraph": "^1.1.0"
  }
}
